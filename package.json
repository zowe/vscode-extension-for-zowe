        {
          "when": "view == zowe.uss.explorer && viewItem =~ /^directory.*/",
          "command": "zowe.uss.createFile",
          "group": "1_create"
        },
        {
          "when": "view == zowe.uss.explorer && viewItem =~ /^directory.*/",
